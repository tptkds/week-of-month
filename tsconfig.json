{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "Node10",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
